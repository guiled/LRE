{
  "name": "letsroleenhanced",
  "version": "7.0.0",
  "main": "src/lre.ts",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@stylistic/eslint-plugin-ts": "^2.10.1",
    "@swc/cli": "^0.1.57",
    "@swc/core": "1.3.16",
    "@types/jest": "29.5",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "colors": "^1.4.0",
    "cspell": "^8.8.4",
    "diff": "^7.0.0",
    "esbuild": "^0.21.5",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-silent-reporter": "^0.6.0",
    "json-colorizer": "^3.0.1",
    "prettier": "^3.3.3",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.1",
    "ts-unused-exports": "^10.1.0",
    "tsx": "^4.15.4",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.12.2"
  },
  "scripts": {
    "test": "jest",
    "testswc": "npx tsx test.ts",
    "count_output": "echo \"Number of lines in build/lre.js $(cat build/lre.js | wc -l) ($(cat build/lre.js | wc -c) caracters)\"",
    "build": "f() { tsx build.ts $@ && yarn count_output; }; f",
    "buildcopy": "f() { tsx build.ts $@ && (cat build/lre.js | pbcopy); }; f",
    "cspell": "npx cspell --no-progress --no-summary --gitignore .",
    "testsilent": "jest --reporters jest-silent-reporter --collectCoverage=false",
    "tscheck": "tsc --noEmit",
    "checkall": "yarn ts-unused-exports tsconfig.json && yarn cspell && yarn tscheck && yarn eslint && yarn testsilent"
  }
}
