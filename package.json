{
  "name": "letsroleenhanced",
  "version": "7.0.2",
  "main": "src/lre.ts",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@swc/cli": "^0.7.3",
    "@swc/core": "1.3.16",
    "@types/jest": "29.5",
    "@types/node": "^22.15.3",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "colors": "^1.4.0",
    "cspell": "^8.19.3",
    "diff": "^7.0.0",
    "esbuild": "^0.25.3",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-silent-reporter": "^0.6.0",
    "json-colorizer": "^3.0.1",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.1",
    "ts-unused-exports": "^11.0.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  },
  "config": {
    "outLREFile": "build/lre.js",
    "outLRETestFile": "build/lre-test.js"
  },
  "scripts": {
    "test": "jest",
    "testswc": "npx tsx test.ts",
    "count_output": "echo \"Number of lines in $npm_package_config_outLREFile $(cat $npm_package_config_outLREFile | wc -l) ($(cat $npm_package_config_outLREFile | wc -c) caracters)\"",
    "build": "f() { tsx build.ts --output $npm_package_config_outLREFile && node --check $npm_package_config_outLREFile && yarn count_output; }; f",
    "builddebug": "f() { tsx build.ts --output $npm_package_config_outLREFile --debug; }; f",
    "buildtest": "f() { tsx build.ts --input src/test.lre.ts --output $npm_package_config_outLRETestFile --debug; }; f",
    "buildall": "yarn build && yarn builddebug && yarn buildtest",
    "spell": "yarn cspell --no-progress --no-summary --gitignore .",
    "testsilent": "jest --reporters jest-silent-reporter --collectCoverage=false",
    "tscheck": "tsc --noEmit",
    "checkall": "yarn ts-unused-exports tsconfig.json && yarn spell && yarn tscheck && yarn eslint && yarn testsilent"
  }
}
